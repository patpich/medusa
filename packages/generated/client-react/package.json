{
  "name": "@medusajs/client-react",
  "version": "0.1.0",
  "description": "React for Medusa Commerce Rest API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "build": "yarn prebuild && tsup src/index.ts",
    "prebuild": "ts-node ./scripts/build.ts && yarn prettier",
    "prettier": "prettier --write --loglevel warn ./src/lib",
    "test": "jest --passWithNoTests"
  },
  "author": "Medusa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/medusajs/medusa",
    "directory": "packages/generate/client-react"
  },
  "bugs": {
    "url": "https://github.com/medusajs/medusa/issues"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.22.0",
    "react": "^16.8 || ^17.0 || ^18.0"
  },
  "dependencies": {
    "@medusajs/client-js": "workspace:*",
    "@medusajs/client-types": "workspace:*",
    "rxjs": "7.5.7"
  },
  "devDependencies": {
    "@medusajs/medusa-oas-cli": "workspace:*",
    "cross-env": "^7.0.3",
    "execa": "^5.1.1",
    "jest": "^27.4.7",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  }
}
