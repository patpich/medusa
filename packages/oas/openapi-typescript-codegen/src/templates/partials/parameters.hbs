{{~#if parameters}}
	{{~#if parametersPath}}

		{{#each parametersPath}}
  			{{{name}}}{{>isRequired}}: {{>type}}{{#if default}} = {{{default}}}{{/if}}{{#unless @last}},
			{{/unless}}
		{{~/each}}
	{{~/if}}
	{{~#if parametersBody}}
		{{~#if parametersPath}},{{/if}}
		{{#with parametersBody}}
			{{{name}}}: {{>type}}
		{{~/with}}
		{{~#if parametersQuery}},{{/if}}
	{{~/if}}
	{{~#if parametersQuery}}
		{{~#if parametersPath}}{{#unless parametersBody}},{{/unless}}{{/if}}
		{{#if codegen.params}}
			queryParams: {{codegen.params}}
		{{~else~}}
			queryParams: {
				{{#each parametersQuery}}
					{{#ifdef description deprecated}}
					/**
					{{#if description}}
					 * {{{escapeComment description}}}
					{{/if}}
					{{#if deprecated}}
					 * @deprecated
					{{/if}}
					 */
					{{/ifdef}}
					{{{prop}}}{{>isRequired}}: {{>type}},
				{{/each}}
			}
		{{~/if}}
	{{~/if}}
{{~/if}}
